{
  "sourceMaps": "inline",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "8.10"
        }
      }
    ]
  ],
  "plugins": ["source-map-support"],
  "env": {
    "production": {
      "sourceMaps": "both",
      "plugins": ["source-map-support"]
    }
  }
}
